# Copy this file to .env and adjust values per environment
# Vite exposes environment variables with the VITE_ prefix at build time

# API configuration
VITE_API_BASE_URL=http://localhost:8001/api
VITE_USE_MOCK=false

# Branding & layout
VITE_APP_NAME=Database Assistant
VITE_APP_FOOTER=Database Assistant
VITE_APP_RELEASE=Release 0.1.0

# UI defaults
VITE_DEFAULT_THEME=system
VITE_DEFAULT_PALETTE=green-07